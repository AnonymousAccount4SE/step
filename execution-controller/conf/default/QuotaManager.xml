<QuotaManagerConfig>
  <id>TestStep</id>
  <quotas>
    <Quota>
	  <id>quotaManagerTest</id>
      <quotaKeyFunction><![CDATA[(input.parameters['quotaManagerTest']==null||input.parameters['quotaManagerTest']=='pass')?null:'']]></quotaKeyFunction>
      <permits>0</permits>
      <acquireTimeoutMs>0</acquireTimeoutMs>
    </Quota>
    <Quota>
	  <id>QFTLicences</id>
	  <description>Limits the number of QFTLicense usage</description>
      <quotaKeyFunction><![CDATA['Licences']]></quotaKeyFunction>
      <permits>10</permits>
    </Quota>
    <Quota>
	  <id>TestInterrupt</id>
      <quotaKeyFunction><![CDATA[input.parameters['TestInterrupt']==null?null:'']]></quotaKeyFunction>
      <permits>0</permits>
      <acquireTimeoutMs>15000</acquireTimeoutMs>
    </Quota>	
  </quotas>
</QuotaManagerConfig>